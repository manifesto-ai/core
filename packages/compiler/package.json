{
  "name": "@manifesto-ai/compiler",
  "version": "1.5.0",
  "description": "Manifesto Compiler - MEL (Manifesto Expression Language) to DomainSchema compiler",
  "author": "eggplantiny <eggplantiny@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifesto-ai/core.git",
    "directory": "packages/compiler"
  },
  "bugs": {
    "url": "https://github.com/manifesto-ai/core/issues"
  },
  "homepage": "https://github.com/manifesto-ai/core/tree/main/packages/compiler#readme",
  "keywords": [
    "manifesto",
    "compiler",
    "dsl",
    "semantic",
    "domain-schema",
    "deterministic",
    "schema-generation",
    "ai"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "default": "./dist/vite.js"
    },
    "./loader": {
      "types": "./dist/loader.d.ts",
      "import": "./dist/loader.js",
      "require": "./loader.cjs",
      "default": "./dist/loader.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "example": "npx tsx examples/simple-compile.ts",
    "cli": "node ./dist/cli/index.js"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.26.0",
    "@manifesto-ai/core": "^2.0.0",
    "@manifesto-ai/host": "^2.0.0",
    "openai": "^4.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "webpack": "^5.0.0",
    "zod": "^4.3.6"
  },
  "peerDependenciesMeta": {
    "@anthropic-ai/sdk": {
      "optional": true
    },
    "openai": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "ink": "^6.6.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "meow": "^14.0.0",
    "react": "^19.2.4"
  },
  "devDependencies": {
    "@anthropic-ai/sdk": "^0.74.0",
    "@manifesto-ai/core": "workspace:*",
    "@types/node": "^25.2.0",
    "@types/react": "^19.2.10",
    "dotenv": "^17.2.3",
    "openai": "^6.17.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "webpack": "^5.102.0",
    "zod": "^4.3.6"
  },
  "files": [
    "dist",
    "loader.cjs"
  ]
}
