    "test:all": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "@manifesto-ai/intent-ir": ">=0.2.0"
  },
  "devDependencies": {
    "@manifesto-ai/intent-ir": "workspace:*",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
