{
  "name": "@manifesto-ai/llm-babybench",
  "version": "1.0.0",
  "description": "BabyBench - LLM agent benchmark suite for Manifesto systems",
  "author": "eggplantiny <eggplantiny@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manifesto-ai/core.git",
    "directory": "apps/llm-babybench"
  },
  "bugs": {
    "url": "https://github.com/manifesto-ai/core/issues"
  },
  "homepage": "https://github.com/manifesto-ai/core/tree/main/apps/llm-babybench#readme",
  "keywords": [
    "manifesto",
    "babybench",
    "benchmark",
    "llm",
    "agent",
    "evaluation",
    "necessity",
    "governance"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "babybench": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "tasks"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "babybench": "node --loader ts-node/esm src/cli/index.ts",
    "download": "npx tsx scripts/download-dataset.ts",
    "download:info": "npx tsx scripts/download-dataset.ts --info",
    "test:task": "npx tsx scripts/test-task.ts",
    "interactive": "npx tsx examples/interactive-mode.ts",
    "benchmark": "npx tsx examples/run-benchmark.ts",
    "benchmark:hybrid": "npx tsx examples/run-benchmark.ts --agent hybrid",
    "benchmark:openai": "npx tsx examples/run-benchmark.ts --agent openai",
    "benchmark:quick": "npx tsx examples/run-benchmark.ts --limit 10",
    "benchmark:lab": "npx tsx scripts/run-lab-benchmark.ts",
    "benchmark:lab:bfs": "npx tsx scripts/run-lab-benchmark.ts --actor bfs",
    "benchmark:lab:llm": "npx tsx scripts/run-lab-benchmark.ts --actor llm",
    "benchmark:lab:quick": "npx tsx scripts/run-lab-benchmark.ts --iterations 2 --config predict",
    "analyze": "npx tsx scripts/analyze-dataset.ts",
    "lab": "npx tsx examples/run-babyai-lab.ts"
  },
  "dependencies": {
    "@manifesto-ai/builder": "workspace:*",
    "@manifesto-ai/core": "workspace:*",
    "@manifesto-ai/host": "workspace:*",
    "@manifesto-ai/lab": "workspace:*",
    "@manifesto-ai/world": "workspace:*",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.0",
    "openai": "^4.0.0",
    "yaml": "^2.3.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/react": "^18.3.12",
    "ink": "^5.0.1",
    "react": "^18.3.1",
    "typescript": "^5.7.3",
    "vitest": "^4.0.16"
  }
}
